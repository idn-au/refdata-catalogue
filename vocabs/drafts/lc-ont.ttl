PREFIX schema: <https://schema.org/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>

# Ontology header

ont:
    a owl:Ontology ;
    schema:name "Local Contexts Label Extension Vocabulary" ;
    schema:creator <https://kurrawong.ai> ;
    schema:publisher <https://linked.data.gov.au/org/idn> ;
    schema:dateCreated "2025-09-23"^^xsd:date ;
    schema:dateModified "2026-02-17"^^xsd:date ;
    schema:dateIssued "2026-03-31"^^xsd:date ;
    schema:version "0.2" ;
    owl:versionIRI :0.2 ;
    schema:description "Defines TKLabel and BCLabel classes as subclasses of skos:Concept, and lc:templateText for associating reusable boilerplate text with Local Contexts labels." ;
    skos:historyNote """2026-02 NJC: made OntPub-conformant

2025-09 LK: created""" ;
    schema:codeRepository "https://github.com/idn-au/refdata-catalogue"^^xsd:anyURI ;
    schema:license <http://purl.org/NET/rdflicense/cc-by4.0> ;
.

<https://linked.data.gov.au/org/idn>
    a schema:Organization ;
    schema:name "Indigenous Data Network" ;
    schema:url "https://idnau.org/"^^xsd:anyURI ;
.

<https://kurrawong.ai>
    a schema:Organization ;
    schema:name "KurrawongAI" ;
    schema:url "https://kurrawong.ai"^^xsd:anyURI ;
.


:TKLabel a owl:Class ;
    rdfs:subClassOf skos:Concept ;
    schema:name "Traditional Knowledge Label" ;
    schema:description "A SKOS Concept representing a Traditional Knowledge Label from the Local Contexts framework." ;
    rdfs:isDefinedBy ont: ;
.

:BCLabel a owl:Class ;
    rdfs:subClassOf skos:Concept ;
    schema:name "Biocultural Label" ;
    schema:description "A SKOS Concept representing a Biocultural Label from the Local Contexts framework." ;
    rdfs:isDefinedBy ont: ;
.

:CCNotice a owl:Class ;
    rdfs:subClassOf skos:Concept ;
    schema:name "Collections Care Notices" ;
    schema:description "A SKOS Concept representing a Collection Care Notice from the Local Contexts framework." ;
    rdfs:isDefinedBy ont: ;
.

:CollectionsCareDuty
  a odrl:Duty ;
  odrl:comment "You are entering a space where Collections Care Notices are in use. Be prepared to follow the protocols for access, use, handling, and viewing associated with certain items or collections. These are in place for the care of the collections and the communities to whom they belong. For more information about Collections Care Notices, see localcontexts.org." ;
  rdfs:isDefinedBy ont: ;
.

odrl:Duty
	a owl:Class ;
	schema:name "Duty" ;
    schema:description "The obligation to perform an Action" ;
    rdfs:isDefinedBy <http://www.w3.org/ns/odrl/2/> ;
.

odrl:Policy
	a owl:Class ;
	schema:name "Policy" ;
	schema:description "A non-empty group of Permissions and/or Prohibitions." ;
	skos:note "A Policy may contain multiple Rules." ;
	rdfs:isDefinedBy <http://www.w3.org/ns/odrl/2/> ;
.

:templateText a owl:DatatypeProperty ;
    rdfs:domain :TKLabel , :BCLabel , :CCNotice ;
    rdfs:range rdf:langString ;
    schema:name "Template Text" ;
    schema:description "A prescriptive text string associated with a Label that provides boilerplate wording intended to be reused or customised by end-users. Distinct from skos:example, which is illustrative only." ;
    rdfs:isDefinedBy ont: ;
.
