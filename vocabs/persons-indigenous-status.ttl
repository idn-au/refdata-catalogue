PREFIX : <https://data.idnau.org/pid/vocab/persons-indigenous-status/>
PREFIX cs: <https://data.idnau.org/pid/vocab/persons-indigenous-status>
PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX prov: <http://www.w3.org/ns/prov#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX reg: <http://purl.org/linked-data/registry#>
PREFIX schema: <https://schema.org/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

:indigenous
    a skos:Concept ;
    dcterms:provenance "Created for the IDN project, 2025"@en ;
    rdfs:isDefinedBy cs: ;
    skos:definition "The individual is an Indigenous individual" ;
    skos:inScheme cs: ;
    skos:prefLabel "Indigenous"@en ;
.

:nonIndigenous
    a skos:Concept ;
    dcterms:provenance "Created for the IDN project, 2025"@en ;
    rdfs:isDefinedBy cs: ;
    skos:definition "The individual is not an Indigenous individual" ;
    skos:inScheme cs: ;
    skos:prefLabel "Non-Indigenous"@en ;
.

:indigenousStatusUnknown
    a skos:Concept ;
    dcterms:provenance "Created for the IDN project, 2025"@en ;
    rdfs:isDefinedBy cs: ;
    skos:definition "The individual's Indigenous status is unknown" ;
    skos:inScheme cs: ;
    skos:prefLabel "Indigenous status Unknown"@en ;
.


<https://linked.data.gov.au/org/idn>
    a schema:Organization ;
    schema:description "The Indigenous Data Network (IDN) was established in 2018 to support and coordinate the governance of Indigenous data for Aboriginal and Torres Strait Islander peoples and empower Aboriginal and Torres Strait Islander communities to decide their own local data priorities."@en ;
    schema:name "Indigenous Data Network" ;
    schema:url "https://mspgh.unimelb.edu.au/centres-institutes/onemda/research-group/indigenous-data-network"^^xsd:anyURI ;
.

cs:
    a skos:ConceptScheme ;
    dcterms:created "2025-05-14"^^xsd:date ;
    dcterms:creator <https://linked.data.gov.au/org/idn> ;
    dcterms:license "https://creativecommons.org/licenses/by/4.0/"^^xsd:anyURI ;
    dcterms:modified "2025-05-14"^^xsd:date ;
    dcterms:publisher <https://linked.data.gov.au/org/idn> ;
    dcterms:rights "(c) Indigenous Data Network, 2025" ;
    reg:status <https://linked.data.gov.au/def/reg-statuses/experimental> ;
    dcterms:provenance "Created during the generation of the IDN's Data Agents Catalogue, based on IDN staff's perceptions of the Indigenous status of persons" ;
    skos:definition "The Indigenous status of a person."@en ;
    skos:hasTopConcept
        :indigenous,
        :nonIndigenous,
        :indigenousStatusUnknown ;
    skos:prefLabel "Indigenous Status of Persons"@en ;
    prov:qualifiedDerivation [
        a prov:Derivation;
        prov:entity cs: ;
        prov:hadRole <https://linked.data.gov.au/def/vocdermods/none> ;
    ] ;
.
