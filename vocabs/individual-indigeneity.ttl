PREFIX : <https://data.idnau.org/pid/vocab/individual-indigeneity/>
PREFIX cs: <https://data.idnau.org/pid/vocab/individual-indigeneity>
PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX prov: <http://www.w3.org/ns/prov#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX reg: <http://purl.org/linked-data/registry#>
PREFIX sdo: <https://schema.org/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

:indigeneity-unknown
    a skos:Concept ;
    dcterms:provenance "Created for the IDN project, 2025"@en ;
    rdfs:isDefinedBy cs: ;
    skos:definition "The individual's indigeneity is unknown" ;
    skos:inScheme cs: ;
    skos:prefLabel "Indigeneity Unknown"@en ;
.

:indigenous
    a skos:Concept ;
    dcterms:provenance "Created for the IDN project, 2025"@en ;
    rdfs:isDefinedBy cs: ;
    skos:definition "The individual is an indigenous individual" ;
    skos:inScheme cs: ;
    skos:prefLabel "Indigenous"@en ;
.

:non-indigenous
    a skos:Concept ;
    dcterms:provenance "Created for the IDN project, 2025"@en ;
    rdfs:isDefinedBy cs: ;
    skos:definition "The individual is not an indigenous individual" ;
    skos:inScheme cs: ;
    skos:prefLabel "Non-Indigenous"@en ;
.

<https://linked.data.gov.au/org/idn>
    a sdo:Organization ;
    sdo:description "The Indigenous Data Network (IDN) was established in 2018 to support and coordinate the governance of Indigenous data for Aboriginal and Torres Strait Islander peoples and empower Aboriginal and Torres Strait Islander communities to decide their own local data priorities."@en ;
    sdo:name "Indigenous Data Network" ;
    sdo:url "https://mspgh.unimelb.edu.au/centres-institutes/centre-for-health-equity/research-group/indigenous-data-network"^^xsd:anyURI ;
.

cs:
    a skos:ConceptScheme ;
    dcterms:created "2025-01-13"^^xsd:date ;
    dcterms:creator <https://linked.data.gov.au/org/idn> ;
    dcterms:license "https://creativecommons.org/licenses/by/4.0/"^^xsd:anyURI ;
    dcterms:modified "2025-01-13"^^xsd:date ;
    dcterms:provenance "Created for the IDN project in 2025."@en ;
    dcterms:publisher <https://linked.data.gov.au/org/idn> ;
    dcterms:rights "(c) Indigenous Data Network, 2025" ;
    reg:status <https://linked.data.gov.au/def/reg-statuses/experimental> ;
    skos:definition "The categorization of an indiviuals indigenouos status."@en ;
    skos:hasTopConcept
        :indigeneity-unknown ,
        :indigenous ,
        :non-indigenous ;
    skos:prefLabel "Individual Indigeneity"@en ;
    prov:qualifiedDerivation
        [
            prov:entity cs: ;
            prov:hadRole <https://linked.data.gov.au/def/vocdermods/none> ;
        ] ;
.
